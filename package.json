{
  "name": "express-react-static",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "dev": "yarn dev:ui & yarn dev:server",
    "build": "yarn build:ui && yarn build:server",
    "dev:ui": "react-scripts start",
    "dev:server": "nodemon",
    "build:ui": "react-scripts build",
    "build:server": "mv tsconfig.json tmp.json && cp tsconfig-server.json tsconfig.json && tsc && rm tsconfig.json && mv tmp.json tsconfig.json",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "yarn lint:ui & yarn lint:server",
    "lint:ui": "eslint src/**/*.ts{,x} --fix",
    "lint:server": "eslint server/**/*.ts --fix"
  },
  "dependencies": {
    "@chakra-ui/react": "^2.7.0",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.34",
    "@types/react": "^18.2.9",
    "@types/react-dom": "^18.2.4",
    "dotenv": "^16.1.4",
    "express": "^4.18.2",
    "framer-motion": "^10.12.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "ts-node": "^10.9.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
